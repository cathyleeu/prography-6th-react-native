{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
          "@assets": "./src/assets",
          "@components": "./src/components",
          "@navigations": "./src/navigations",
          "@scenes": "./src/scenes",
          "@helpers": "./src/helpers",
          "@styles": "./src/styles"
        }
      }
    ]
  ]
}